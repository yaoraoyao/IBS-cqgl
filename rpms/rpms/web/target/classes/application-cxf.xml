<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jaxws="http://cxf.apache.org/jaxws"
       xsi:schemaLocation=" http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-4.2.xsd
       http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">
    <import resource="classpath:META-INF/cxf/cxf.xml"/>
    <import resource="classpath:META-INF/cxf/cxf-servlet.xml"/>
    <!--配置成spring的bean
    id="userService"  beanId
    implementor="cn.itsource.service.impl.UserServiceImpl"   接口实现类
    address="/HelloWorld"  服务地址
    -->
    <bean id="hello" class="cn.itsource.web.webservice.impl.SettedWebServiceImpl" />
    <jaxws:endpoint id="userService" implementor="#hello" address="/helloworld"/>
</beans>

